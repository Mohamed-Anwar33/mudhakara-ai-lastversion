{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "crons": [
        {
            "path": "/api/process-queue",
            "schedule": "0 3 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ]
}
